<h2>Create Applicant User</h2>

<%= form_for @applicant, url: { action: "create" }  do |f| %>
  <div class="field">
    <%= f.label :first_name %>
    <%= f.text_field :first_name, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>

  <div class="actions">
    <%= f.submit "Create" %>
  </div>
<% end %>
